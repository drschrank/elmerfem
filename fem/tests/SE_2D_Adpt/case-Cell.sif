Check Keywords "Warn"

Header :: Mesh DB "." "Cell"

Constants
  alkali wavelength = Real 794.7e-9
  alkali frequency width = Real 14e9 !Real 126.65D9
  alkali temperature = Real 800

  laser wavelength = Real 794.7e-9
  laser line width = Real 1.0e-10
  !beta = Real 1
  Compute Beta = LOGICAL True

  Compute Laser Power = LOGICAL True

  Laser Spot Size = Real 122.5e-4

  cell radius = Real .03
  T1 = Real 600
  T1 Pressure = real 1e5
  T1 Temperature = real 300
End

Simulation
  Max Output Level = 5
  Coordinate System = Cartesian
  Coordinate Scaling = 0.5
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 20
  Post File   = "cell.vtu"
End

Body 1
  Equation = 1
  Material = 1
  Initial Condition = 1
  Body Force = 1
End

Material 1
  !alkali density = Real MATC
  alkali density = Variable Concentration; Real MATC "tx"  


  !spin destruction rate = Real 3
  !spin destruction rate = Real 2.1D4
  spin destruction rate = Variable Concentration, Temperature, Pressure; Real Procedure "OPSolver" "CalculateSpinDestructionRate"
 
  rubidium polarization = Variable optrate, spin destruction rate; Real Procedure "OPSolver" "CalculateRbPol"

  alkali-alkali spin destruction rate = Real 3.9e-14

  xe spin destruction rate = Real 2.44e5

  Helium Spin Destruction Term Included= LOGICAL False
  Nitrogen Spin Destruction Term Included = LOGICAL True
  Xenon Spin Destruction Term Included = LOGICAL True
  vander Wall Spin Destruction Term Included = LOGICAL True

  xe fraction = Real 0.5
  n2 fraction = Real 0.5
  he fraction = Real 0

  
  binary exchange rate = Real 3e-16

  spin exchange rate = Variable Concentration; Real Procedure "OPSolver" "CalculateSpinExchangeRate"
  spin relaxation rate = Variable Pressure, Temperature; Real Procedure "OPSolver" "CalculateSpinDestructionRate"

  laser direction 1 = Real -1
  laser direction 2 = Real 0 

  convert density = LOGICAL False
  Density = 1
  Concentration Diffusivity = Real 1.0e-3

  XePol Diffusivity = Variable Pressure, Temperature; Real Procedure "OPSolver" "CalculateXenonDiffusion"

  Gamma = Variable Concentration, Pressure, Temperature; Real Procedure "OPSolver" "CalculateXeGamma" 
End

Solver 1
  Equation = "OPSolver"
  Variable = "optrate"
  Procedure = "OPSolver" "OPSolver"
  Element = "p:2"
  Linear System Solver = Direct
  Steady State Convergence Tolerance = 1e-9
  Nonlinear System Max Iterations = 20
  Nonlinear System Convergence Tolerance = 1e-9

End

Solver 2
Equation = Advection Diffusion
  Variable = Concentration
  Variable DOFs = 1
  Procedure = "AdvectionDiffusion" "AdvectionDiffusionSolver"
  Exec Solver = Never
End

Solver 3
  Equation = Navier-Stokes
  Flow Model = Full
  Procedure = "FlowSolve" "FlowSolver"
  Variable = Flow Solution[Velocity:3 Pressure:1]
  Exec Solver = Never
End

Solver 4
  Equation = Heat Equation
  Variable = Temperature
  Steady State Convergence Tolerance = 1e-4
  Exec Solver = Never
End

Solver 5
  Equation = XePol
  Variable = XePol
  Exported Variable 1 = XePol Field Source
  Procedure = "XePol" "XePol"
  Exec Solver = Always
  Nonlinear Update Exported Variables = True
  Stabilize = True
  Bubbles = False
  Lumped Mass Matrix = False
  Optimize Bandwidth = True
  Steady State Convergence Tolerance = 1e-4
  Nonlinear System Convergence Tolerance = 1.0e-5
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 1000
  Nonlinear System Newton After Tolerance = 1.0e-7
  Nonlinear System Relaxation Factor = 1
  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStabl
  Linear System Max Iterations = 5000
  Linear System Convergence Tolerance = 1.0e-7
  BiCGstabl polynomial degree = 3
  Linear System Preconditioning = ILU2
  Linear System ILUT Tolerance = 1.0e-3
  Linear System Abort Not Converged = True
  Linear System Residual Output = 2
  Linear System Precondition Recompute = 1

  Apply Limiter = Logical True
End

Solver 6
  Exec Solver = Always
  Procedure = File "SaveData" "SaveMaterials"
  Parameter 1 = String spin destruction rate
  Parameter 2 = String Gamma
  Parameter 3 = String XePol Diffusivity
  Parameter 4 = String rubidium polarization
  Parameter 5 = String spin exchange rate
  Parameter 6 = String spin relaxation rate
End

Equation 1
  Active Solvers(6) = 1 2 3 4 5 6
  XePol Convection = String None
End

Body Force 1
  XePol Field Source =  Variable Concentration,Temperature,Pressure,optrate; Real Procedure "OPSolver" "CalculateXeField"  

  XePol Lower Limit = Real 1e-6
  XePol Upper Limit = Real 1
End

Boundary Condition 1
  Target Boundaries(1) = 1
  !optrate = Real 1
  !optrate = Real 62359
  Laser Power = Real 150
  !Concentration = Variable optrate 
  !Procedure "OPSolver" "calculaterbmumdensitycm"

  Pressure = Variable Temperature; MATC "5e5*tx/300"

  Temperature = 800

  Decay Rate Coefficient = Variable Pressure, Temperature; Real Procedure "OPSolver" "CalculateDecayRate"
End

Boundary Condition 2
  Target Boundaries(1) = 2
     Decay Rate Coefficient = Variable Pressure, Temperature; Real Procedure "OPSolver" "CalculateDecayRate"

  Pressure = Variable Temperature; MATC "5e5*tx/300"

  Temperature = 800
End

Initial Condition 1
  !optrate = Real 1
  !optrate = Real 62359

  Concentration = Variable optrate 
  Procedure "OPSolver" "calculaterbmumdensitycm"

  Pressure = Variable Temperature; MATC "5e5*tx/300"

  Temperature = 800
End

!Solver 1 :: Reference Norm = Real 1.0578593
!Solver 1 :: Reference Norm Tolerance = Real 1.0E-5
!RUN
