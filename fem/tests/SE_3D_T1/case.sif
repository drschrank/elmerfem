Check Keywords "Warn"

Header :: Mesh DB "." "sphere"

Simulation
  Max Output Level = 5
  Coordinate System = Cartesian
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
  Post File   = "case.vtu"
End

Body 1
  Equation = 1
  Material = 1
  Body Force = 1
End

Material 1
  Convection Velocity 1 = 0
  Convection Velocity 2 = 0

  xe diffusivity = Real 1.0e-5

  alkali polarization = Real 1.0
  spin relaxation rate = Real 1.0
  spin exchange rate = Real 1.0

  
End

!Body Force 1 :: Field Source = Real 1
Equation 1 :: Active Solvers(1) = 1

Solver 1
  Equation = "SESolver"
  Variable = "XePol"
  Procedure = "SESolver" "SESolver"
  Element = "p:2"
  Linear System Solver = Direct
  Steady State Convergence Tolerance = 1e-9
End

Boundary Condition 1
  Target Boundaries(1) = 1
  T1 Coefficient = Real 1

End

Boundary Condition 2
  Target Boundaries(1) = 2
  T1 Coefficient = Real 1

End

Boundary Condition 3
  Target Boundaries(2) = 3 4
  T1 Coefficient = Real 1

End
!Solver 1 :: Reference Norm = Real 1.0578593
!Solver 1 :: Reference Norm Tolerance = Real 1.0E-5
!RUN
